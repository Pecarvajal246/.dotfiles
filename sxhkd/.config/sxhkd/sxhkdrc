super + ctrl + t
	twofi

super + ctrl + s
  flameshot gui

super + p
	rofi -show run -modi 'run,drun,powermenu:~/.local/bin/rofi-power-menu' 

super + y
	streams.sh

super + e
	thunar

super + b
	firefox

XF86Audio{Prev,Next}
	playerctl {prev,next}

XF86AudioPlay
	playerctl play-pause

ctrl + space
  dunstctl close-all
